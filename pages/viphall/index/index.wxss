/*搜索框*/
.seach{width: 100%;height: 60rpx;background: #f5f5f5;padding: 14rpx 0; border-bottom: 2rpx solid #dedede;position: fixed;left: 0;top: 0;right: 0;z-index: 100;}
.seach_input{ margin: 0 20rpx;background: #fff;height: 60rpx; border-radius: 10rpx;position: relative;}
.search_text{float: left;margin-left: 10rpx;background: url(https://webapi.jsjinfo.cn/smallApp/viphall/search_copy.png) no-repeat left;background-size: 28rpx 27rpx;}
.input_val{width: 100%; height: 50rpx; line-height: 50rpx;margin-left: 38rpx;font-size: 24rpx;color: #333;}

/*贵宾厅列表展示*/
.list{ margin: 0 auto; width: 95%;}
.vip_list_group{margin:0 auto;width:95%;font-size:13px;}
.vip_name{color:#666;font-weight:bold;height:35px;line-height:35px;border-bottom:1px solid #DEDEDE;font-size:15px;}
.vip_list{margin-top:10px;margin-left:10px; border-bottom: 5px solid #EEEEEE; padding-bottom:10px;overflow: hidden; position: relative;min-height:85px;}
.vip_img{float:left;width:95px;height:85px;border:0;vertical-align:middle;border-radius:5px; position: absolute; top: 0; left: 0;}
.vip_text_price{font-size:15px;color: #FFB759;margin-left:5rpx;float: left;}
.icoimg{width:11px;height:12px;vertical-align:middle;border:0;margin-right:10px;}
.vip_info{line-height:20px; position: absolute; top: 0; left: 105px; right: 0; bottom: 0;}
.vip_text1{ float: right;}
